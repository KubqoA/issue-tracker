{
    "name": "kubqoa/issuetracker",
    "description": "Easily create and publish issues from your Laravel app to your git repository hosted on supported platform",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Arbet",
            "email": "arbetjakub@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "mockery/mockery": "dev-master",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "KubqoA\\IssueTracker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KubqoA\\IssueTracker\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "styles": "php-cs-fixer fix --config=.php_cs.dist -v --using-cache=no src"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "KubqoA\\IssueTracker\\IssueTrackerServiceProvider"
            ]
        }
    }
}
